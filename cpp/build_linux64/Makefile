# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release_linux64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_linux64)
  pytiledb_config = release_linux64
endif

PROJECTS := pytiledb

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

pytiledb:
ifneq (,$(pytiledb_config))
	@echo "==== Building pytiledb ($(pytiledb_config)) ===="
	@${MAKE} --no-print-directory -C . -f pytiledb.make config=$(pytiledb_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f pytiledb.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_linux64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   pytiledb"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"